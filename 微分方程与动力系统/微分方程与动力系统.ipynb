    "import numpy as np\n",
    "from scipy.integrate import quad\n",
    "\n",
    "# 定义函数\n",
    "def f(x):\n",
    "    return np.cos(2*np.pi*x)*np.exp(-x)+1.2\n",
    "#计算定积分\n",
    "integral,error = quad(f,0,0.7)\n",
    "\n",
    "print(f'定积分的结果是：{integral}')\n"
   ]
